@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :height_of ;
  lemon:entry    :of .

:height_of a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :height_form ;
  lemon:synBehavior    :height_of_nounpp ;
  lemon:sense          :height_sense1_ontomap ; 
  lemon:sense          :height_sense2_ontomap .

:height_form a     lemon:Form ;
  lemon:writtenRep "height"@en .

:height_of_nounpp a            lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:height_sense1_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping     :height_sense1_ontomap ;
  lemon:reference       <http://dbpedia.org/ontology/height> ;
  lemon:subjOfProp      :arg2 ;
  lemon:objOfProp       :arg1 ;
  lemon:condition       :height_person_condition .

:height_sense2_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping     :height_sense2_ontomap ;
  lemon:reference       <http://dbpedia.org/ontology/height> ;
  lemon:subjOfProp      :arg2 ;
  lemon:objOfProp       :arg1 ;
  lemon:condition       :height_structure_condition .

:height_person_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#double> .

:height_structure_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
