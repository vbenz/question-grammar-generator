@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :child_of ;
  lemon:entry    :of .

:child_of a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :child_form ;
  lemon:otherForm      :children_form ;
  lemon:synBehavior    :child_of_nounpp ;
  lemon:sense          :child_sense_ontomap .

:child_form a      lemon:Form ;
  lemon:writtenRep "child"@en .

:children_form a   lemon:Form ;
  lemon:writtenRep "children"@en ;
  lexinfo:number   lexinfo:plural .

:child_of_nounpp a             lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:child_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping    :child_sense_ontomap ;
  lemon:reference      <http://dbpedia.org/ontology/child> ;
  lemon:subjOfProp     :arg2 ;
  lemon:objOfProp      :arg1 ;
  lemon:condition      :child_condition .

:child_condition a     lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
