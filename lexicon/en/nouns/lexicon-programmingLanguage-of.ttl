@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programmingLanguage_of ;
  lemon:entry    :of .

:programmingLanguage_of a      lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programmingLanguage_form ;
  lemon:synBehavior    :programmingLanguage_of_nounpp ;
  lemon:sense          :programmingLanguage_sense_ontomap .

:programmingLanguage_form a lemon:Form ;
  lemon:writtenRep  "programming language"@en .

:programmingLanguage_of_nounpp a       lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:programmingLanguage_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping          :programmingLanguage_sense_ontomap ;
  lemon:reference            <http://dbpedia.org/ontology/programmingLanguage> ;
  lemon:subjOfProp           :arg2 ;
  lemon:objOfProp            :arg1 ;
  lemon:condition            :programmingLanguage_condition .

:programmingLanguage_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/ProgrammingLanguage> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
